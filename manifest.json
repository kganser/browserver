{
  "manifest_version": 2,
  "name": "browserver",
  "description": "Web server framework",
  "offline_enabled": true,
  "version": "2",
  "minimum_chrome_version": "24",
  "permissions": [{"socket": ["tcp-listen"]}, "unlimitedStorage"],
  "app": {
    "background": {
      "page": "browserver.html"
    }
  }
}